{
    "root": true,
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "parser": "babel-eslint",
        "sourceType": "module"
    },
    "extends": [
        "plugin:vue/recommended", 
        "eslint:recommended"
    ],
    "rules": {
        "vue/order-in-components": "off",
        "comma-dangle": "off",
        "global-require": "off",
        "import/no-extraneous-dependencies": "off",
        "import/no-unresolved": "off",
        "import/extensions": "off",
        "no-alert": "off",
        "no-console": "off",
        "no-bitwise": "off"
    },
    "globals": {
        "jQuery": false,
        "$": false
    }
}